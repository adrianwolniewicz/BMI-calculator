height = float(input("enter your height in m: "))
weight = float(input("enter your weight in kg: "))
bmi = round(weight/height**2)
if bmi <= 18:
  print (f"Your {int(bmi)} you are underweight.")
elif bmi > 19 and bmi <= 22:
  print (f"Your {int(bmi)} you have a normal weight.")
elif bmi > 23 and bmi <= 33:
  print (f"Your {int(bmi)} you are slightly overweight")
elif bmi > 34 and bmi <= 35:
  print (f"Your {int(bmi)} you are obese.")
else:
  print (f"Your {int(bmi)} you are clinically obese")
